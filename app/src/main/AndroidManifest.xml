<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.m7mdabaza.xogame">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <!--  -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="" />

        <activity
            android:name="com.m7mdabaza.xogame.onePlayer.HardLevelVsComputer"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.onePlayer.MediumLevelVsComputer"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.onePlayer.EasyLevelVsComputer"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.twoPlayers.HardLevel"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.twoPlayers.MediumLevel"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.twoPlayers.EasyLevel"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.startActivities.MainActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.m7mdabaza.xogame.startActivities.splash"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
